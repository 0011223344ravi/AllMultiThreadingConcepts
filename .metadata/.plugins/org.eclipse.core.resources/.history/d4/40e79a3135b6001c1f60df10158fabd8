package com;

//case 2 multiple threads are working on multiple objects 

class Disp {
	public void wish (String name) {
	for (int i =0; i<10; i++)
	{
		System.out.println("Welcome "+name);
	}
	try {
		Thread.sleep(2000);
	} catch (InterruptedException e) {
		// TODO Auto-generated catch block
		e.printStackTrace();
	}
	}
	
}


